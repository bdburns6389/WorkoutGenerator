@model WorkoutGenerator.ViewModels.ViewRecordViewModel

<h2>@Model.Workout.Name</h2>
<ul>
    @foreach (var exercise in Model.ExerciseWorkouts)
    {
        <li>
            <a asp-controller="Record"
               asp-action="ViewRecords"
               asp-route-ExerciseID="@exercise.ExerciseID"
               asp-route-WorkoutID="@exercise.WorkoutID">@exercise.Exercise.Name</a>
        </li>
    }
</ul>