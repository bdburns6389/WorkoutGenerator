<div class="container">
    <h1>@ViewBag.title</h1>

    <form method="post">

        @foreach (var musclegroup in ViewBag.musclegroups)
        {
            <input type="checkbox" name="musclegroupIds" id="@musclegroup.MuscleGroupID" value="@musclegroup.MuscleGroupID" />
            <label for="@musclegroup.MuscleGroupID">@musclegroup.Name</label>
            <br />
        }

        <input type="submit" value="Remove Selected Muscle Group(s)" />
    </form>
</div>